package com.eduardo.solid.models;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public abstract class AccountBase {
    private int id;
    private String name;
    private double balance;

    public AccountBase(int id, String name, double balance) {
        this.id = id;
        this.name = name;
        this.balance = balance;
    }
}
